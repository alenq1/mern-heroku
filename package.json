    "start": "npm run build && node ./build/app/server.js",
    "wsstart": "npm run build && node ./build/app/ws_server.js",
    "devstart": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app/server.ts",
    "dev_wsstart": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app/ws_server.ts",
    "dev_worker_start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app/queue_server.ts",
    "listen": ""
  },
